<%= render 'shared/content_title', title: 'New User' %>

<div class="well">
<%= form_tag do  %>
<div class="control-group">
<%= label_tag :username %>
<%= text_field_tag :username %>
</div>
<div class="control-group">
<%= label_tag :password %>
<%= text_field_tag :password %>
</div>
<div class="control-group">
<%= label_tag :password_confirmation %>
<%= text_field_tag :password_confirmation %>
</div>
<%= submit_tag("Create User", class: "btn btn-default") %>
<% end %>
</div>